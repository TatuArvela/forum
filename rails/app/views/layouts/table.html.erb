<% content_for :content do %>
  <%= yield :table_before %>

  <div class="card overflow-hidden my-4">
    <div class="card-body p-0">
      <table class="table table-striped mb-0">

        <thead class="thead-dark">
        <%= yield :table_head %>
        </thead>

        <tbody>
        <%= yield :table_body %>
        </tbody>

      </table>
    </div>
  </div>

  <%= yield :table_after %>
<% end %>

<%= render template: 'layouts/application' %>
